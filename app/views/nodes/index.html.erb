<h1>Welcome!</h1>

<h2>List of Projects</h2>



<ul id="projects">
<div class="row">

<% @nodes.each do |node| %>

<li> <%=  link_to node.name, node_path(node), class: 'projects_index col-md-3 btn btn-default btn-lg', id: "node_#{node.id}" %> </li>

<% end %>





<li><%= link_to "Create New Project", new_node_path, class: "new_project col-md-3 btn btn-default btn-lg" %> </li>


</ul>

</div>